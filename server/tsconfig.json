{
  "extends": "../tsconfig.json",
  "compilerOptions": {
    "outDir": "../dist",
    "target": "ES2020",
    "module": "ES2020",
    "moduleResolution": "node",
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "skipLibCheck": true,
    "resolveJsonModule": true,
    "sourceMap": false,
    "noEmit": false
  },
  "include": ["./**/*.ts"],
  "exclude": ["node_modules", "**/*.test.ts"]
}

